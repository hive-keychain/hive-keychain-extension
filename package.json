    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "webpack-merge": "5.8.0"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "overrides": {
    "sass-graph": {
      "scss-tokenizer": "$scss-tokenizer"
    }
  },
  "browser": {
    "crypto": false
  },
  "workspaces": [
    "packages/*"
  ]
}
