{
  "name": "hive-keychain",
  "version": "3.10.5",
  "description": "Hive Keychain extension",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "dev": "npm outdated; concurrently \"npm:dev:*\"",
    "dev:chromium": "webpack --config webpack/chromium/webpack.dev.js -w",
    "dev:firefox": "webpack --config webpack/firefox/webpack.dev.js -w",
    "build": "npm outdated; concurrently \"npm:build:*\"",
    "build:chromium": "rm -rf ./dist-prod & webpack --config webpack/chromium/webpack.prod.js",
    "build:firefox": "rm -rf ./dist-prod-firefox & webpack --config webpack/firefox/webpack.prod.js",
    "build:beta": "rm -rf ./dist-beta & webpack --config webpack/chromium/webpack.beta.js ; cp dist-beta/assets/images/iconhive-beta.png dist-beta/assets/images/iconhive.png",
    "analyze": "concurrently \"npm:analyze:*\"",
    "analyze:chromium": "npx webpack --profile --json > stats.json --config webpack/chromium/webpack.prod.js",
    "analyze:firefox": "npx webpack --profile --json > stats.json --config webpack/firefox/webpack.prod.js",
    "crowdin:pull": "crowdin download",
    "crowdin:push": "crowdin upload sources",
    "doc": "documentation build public/hive_keychain.js -f md -c documentation/build/documentation.yml -o documentation/README.md",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions@0.0.10",
    "postinstall": "patch-package",
    "test": "jest --verbose",
    "test:detect": "jest --verbose --detectOpenHandles",
    "test:watch": "jest --verbose --watch",
    "test:cov": "jest --verbose --coverage",
    "bump": "scripts/bump-version",
    "release": "scripts/release.sh",
    "release-beta": "scripts/release-beta.sh",
    "check-installed-libs": "node scripts/check-installed-libs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoodkev/hive-keychain.git"
  },
  "keywords": [
    "hive",
    "keychain",
    "extension"
  ],
  "author": "stoodkev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoodkev/hive-keychain/issues"
  },
  "homepage": "https://github.com/stoodkev/hive-keychain#readme",
  "dependencies": {
    "@engrave/ledger-app-hive": "3.5.6",
    "@hiveio/hive-js": "2.0.4",
    "@hookform/resolvers": "3.1.1",
    "@ledgerhq/hw-transport": "6.28.8",
    "@ledgerhq/hw-transport-webusb": "^6.27.19",
    "@types/chrome": "^0.0.243",
    "@types/crypto-js": "4.1.1",
    "@types/yargs": "17.0.11",
    "axios": "0.28.0",
